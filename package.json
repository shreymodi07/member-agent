{
  "name": "teladoc-member-agents",
  "version": "1.0.0",
  "description": "AI-powered code review and security scanning for Teladoc",
  "main": "dist/index.js",
  "bin": {
    "teladoc-agent": "dist/cli.js"
  },
  "scripts": {
    "build": "tsc",
    "dev": "tsx src/cli.ts",
    "start": "node dist/cli.js",
    "test": "jest",
    "lint": "eslint src/**/*.ts",
    "format": "prettier --write src/**/*.ts",
    "prepare": "npm run build"
  },
  "keywords": [
    "teladoc",
    "ai",
    "code-review",
    "security",
    "vulnerabilities",
    "compliance",
    "cli"
  ],
  "author": "Teladoc Member Team",
  "license": "MIT",
  "dependencies": {
    "@anthropic-ai/sdk": "^0.24.3",
    "@types/node": "^20.0.0",
    "chalk": "^5.3.0",
    "commander": "^11.0.0",
    "fs-extra": "^11.1.0",
    "inquirer": "^9.2.0",
    "openai": "^4.20.0",
    "ora": "^7.0.0",
    "yaml": "^2.3.0"
  },
  "devDependencies": {
    "@types/fs-extra": "^11.0.0",
    "@types/inquirer": "^9.0.0",
    "@types/jest": "^29.0.0",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "eslint": "^8.0.0",
    "jest": "^29.0.0",
    "prettier": "^3.0.0",
    "tsx": "^4.0.0",
    "typescript": "^5.0.0"
  },
  "files": [
    "dist",
    "templates",
    "README.md"
  ],
  "engines": {
    "node": ">=18.0.0"
  }
}
